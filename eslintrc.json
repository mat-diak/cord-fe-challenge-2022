{
  "extends": ["eslint:recommended", "plugin:cypress/recommended", "prettier"],
  "plugins": ["cypress"],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "cypress/globals": true
  }
}